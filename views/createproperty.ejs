<%- include("partials/header")%>
  <div class="mt-10 w-[80%] mx-auto p-10">
    <h1 class="mb-5 text-3xl">This is Create Property</h1>
    <form action="/property" method="post" enctype="multipart/form-data">
      <input class="border" type="text" name="title" placeholder="Title">
      <input class="border" type="text" name="description" placeholder="Description">
      <input class="border" type="number" name="price" placeholder="Price">
      <input class="border" type="text" name="location" placeholder="Location">
      <input class="border" type="file" name="image" placeholder="Image">
      <input class="border" type="status" name="status" placeholder="available | sold | pending">
      <button>Submit</button>
    </form>

  </div>
  <%- include("partials/footer")%>